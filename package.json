{
  "name": "vite-mern-template",
  "version": "1.0.2",
  "main": "backend/src/index.ts",
  "type": "module",
  "scripts": {
    "watch": "tsc --watch",
    "backend": "node build/index.js",
    "dev:be": "concurrently \"npm run watch\" \"npm run backend\"",
    "dev:fe": "npm start --prefix ./frontend",
    "check-types": "tsc",
    "prettier": "prettier --write .",
    "build:backend": "tsc",
    "build:frontend": "npm run build --prefix ./frontend",
    "build": "tsc && npm run build:frontend",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000 --detectOpenHandles"
  },
  "repository": "https://github.com/apicgg/vite-mern-template.git",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-table": "^8.11.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
